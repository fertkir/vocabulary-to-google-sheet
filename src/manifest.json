{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "action": {
    "default_icon": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": [],
    "js": ["sites-settings.js", "content.js"]
  }],
  "icons": {
    "48": "icons/icon48.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "identity",
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "http://localhost/vocabulary-add/*",
    "https://www.googleapis.com/*"
  ]
}