{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-3.4.1.min.js", "sites-settings.js", "content-polyfills.js", "content.js"]
  }],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "identity",
    "storage",
    "webRequest",
    "http://localhost/vocabulary-add/*",
    "https://www.googleapis.com/*"
  ],
  "icons": {
    "48": "icons/icon48.png"
  }
}