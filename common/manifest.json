{
  "manifest_version": 2,
  "name": "Vocabulary saver",
  "version": "0.2.2",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
  {
    "matches": [
      "*://dictionary.cambridge.org/*",
      "*://*.thefreedictionary.com/*",
      "*://*.spanishdict.com/*",
      "*://*.merriam-webster.com/*",
      "*://ru.wiktionary.org/*"
    ],
    "js": ["jquery-3.4.1.min.js"]
  },
  {
    "matches": [
      "*://dictionary.cambridge.org/*"
    ],
    "js": ["sites/cambridge.js"]
  },
  {
    "matches": [
      "*://*.thefreedictionary.com/*"
    ],
    "js": ["sites/freedictionary.js"]
  },
  {
    "matches": [
      "*://*.spanishdict.com/*"
    ],
    "js": ["sites/spanishdict.js"]
  },
  {
    "matches": [
      "*://*.merriam-webster.com/*"
    ],
    "js": ["sites/merriam-webster.js"]
  },
  {
    "matches": [
      "*://ru.wiktionary.org/*"
    ],
    "js": ["sites/ru-wiktionary.js"]
  },
  {
    "matches": [
      "*://dictionary.cambridge.org/*",
      "*://*.thefreedictionary.com/*",
      "*://*.spanishdict.com/*",
      "*://*.merriam-webster.com/*",
      "*://ru.wiktionary.org/*"
    ],
    "js": ["content-polyfills.js", "content.js"]
  }],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}